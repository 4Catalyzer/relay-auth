{
  "name": "relay-network-layer",
  "version": "1.3.2",
  "homepage": "https://github.com/4Catalyzer/relay-network-layer#readme",
  "bugs": {
    "url": "https://github.com/4Catalyzer/relay-network-layer/issues"
  },
  "license": "MIT",
  "author": "4Catalyzer",
  "main": "lib/index.js",
  "module": "lib/es/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/4Catalyzer/relay-network-layer.git"
  },
  "scripts": {
    "build": "4c build src",
    "format": "4c format .",
    "lint": "4c lint .",
    "prepublishOnly": "yarn run build",
    "release": "4c release",
    "tdd": "jest --watch",
    "test": "yarn lint && yarn typecheck && yarn testonly --coverage",
    "testonly": "jest",
    "typecheck": "tsc --noEmit && tsc --noEmit -p test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "yarn 4c lint --fix"
  },
  "prettier": "@4c/prettier-config",
  "jest": {
    "preset": "@4c/jest-preset",
    "moduleNameMapper": {
      "fetch-mock$": "<rootDir>/node_modules/fetch-mock/cjs/client.js"
    },
    "setupFiles": [
      "whatwg-fetch"
    ]
  },
  "release": {
    "conventionalCommits": true
  },
  "dependencies": {
    "@restart/hooks": "^0.3.20",
    "@types/react": "^16.9.17",
    "@types/relay-runtime": ">=8.0.0",
    "@types/store": "^2.0.2",
    "socket.io-client": "^3.0.4",
    "store": "^2.0.12",
    "utility-types": "^3.10.0"
  },
  "devDependencies": {
    "@4c/babel-preset": "^8.0.1",
    "@4c/cli": "^2.1.12",
    "@4c/jest-preset": "^1.5.3",
    "@4c/prettier-config": "^1.1.0",
    "@4c/tsconfig": "^0.3.1",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.15",
    "@types/socket.io-client": "^1.4.34",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "codecov": "^3.8.1",
    "eslint-config-4catalyzer-jest": "^2.0.9",
    "eslint-config-4catalyzer-react": "^1.0.12",
    "eslint-config-4catalyzer-typescript": "^3.0.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fetch-mock": "^9.11.0",
    "fetch-mock-jest": "^1.5.1",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.2",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "relay-runtime": "^10.1.0",
    "typescript": "^4.1.2",
    "whatwg-fetch": "^3.5.0"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "relay-runtime": ">=4.0.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "lib"
  }
}
